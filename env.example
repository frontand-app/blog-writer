# Google AI API Configuration
# Required: At least one of GOOGLE_API_KEY or GEMINI_API_KEY must be set
GOOGLE_API_KEY=your_google_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Vertex AI Configuration (Alternative to Google AI API)
# Set GOOGLE_GENAI_USE_VERTEXAI=true to use Vertex AI instead
GOOGLE_GENAI_USE_VERTEXAI=false
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_LOCATION=global

# Model Configuration (Optional - defaults shown)
CONTENT_MODEL=gemini-2.5-pro
VALIDATOR_MODEL=gemini-2.5-flash
RESEARCH_MODEL=gemini-1.5-pro

# Output Configuration (Optional - defaults shown)
OUTPUT_DIR=output
AGGREGATE_FILE=

# Supabase Configuration (Optional - not currently used in main flow)
SUPABASE_URL=
SUPABASE_KEY=

